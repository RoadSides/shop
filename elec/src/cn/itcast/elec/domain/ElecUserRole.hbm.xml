<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
   <hibernate-mapping>
   			<class name="cn.itcast.elec.domain.ElecUserRole"  table="elec_user_role">
   					<id  name="seqID"  type="integer">
   						<generator class="increment"></generator>
   					</id>
   					<property name="userID"  type="string"></property>
   					<property name="roleID"  type="string"></property>
   					<property name="remark"  type="string"></property>
   			</class>
   </hibernate-mapping>